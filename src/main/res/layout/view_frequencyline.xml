<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center_vertical"
    android:id="@+id/ll_frequencyline_layout"
    android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_frequency"
            android:layout_width="@dimen/frequency_width"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:textColor="@color/frequency"
            android:textSize="@dimen/frequency_textSize" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <com.jerry.nsis.view.BigBed
            android:visibility="visible"
            android:id="@+id/bed10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    <RelativeLayout
        android:id="@+id/rl_paging"
        android:visibility="invisible"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="30dp">

        <ImageView
            android:layout_centerInParent="true"
            android:id="@+id/iv_paging"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/paging" />

        <TextView
            android:layout_centerInParent="true"
            android:id="@+id/tv_paging"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="3dp"
            android:paddingRight="3dp"
            android:textColor="#333333"
            android:textSize="@dimen/bed_paging_textSize" />

    </RelativeLayout>



</LinearLayout>
